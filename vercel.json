{
  "version": 2,
  "rewrites": [
    { "source": "/", "destination": "/dashboard.html" },
    { "source": "/dashboard", "destination": "/dashboard.html" },
    { "source": "/lifestyle", "destination": "/lifestyle.html" },
    { "source": "/(.*)", "destination": "/dashboard.html" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" }
  ]
}